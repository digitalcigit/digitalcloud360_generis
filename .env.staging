# Configuration Environment Staging Genesis AI
ENV=staging

# Database Configuration
DATABASE_URL=postgresql://genesis_user:genesis_pass@genesis-db:5432/genesis_staging

# Redis Configuration  
REDIS_URL=redis://genesis-redis:6379/0

# DigitalCloud360 Integration
DIGITALCLOUD360_API_URL=http://digitalcloud360:8000
DIGITALCLOUD360_SERVICE_SECRET=your_service_secret_here
DIGITALCLOUD360_TIMEOUT=30

# Genesis AI Configuration
GENESIS_AI_SECRET_KEY=your_genesis_secret_key_here
APP_VERSION=1.0.0

# AI Services API Keys (Ã€ configurer sur serveur)
OPENAI_API_KEY=your_openai_key_here
TAVILY_API_KEY=your_tavily_key_here  
LOGOAI_API_KEY=your_logoai_key_here

# Logging
LOG_LEVEL=INFO

# Security
ALLOWED_HOSTS=localhost,127.0.0.1,your-staging-domain.com
